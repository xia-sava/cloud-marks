    "@mui/material": "^5.16.0",
    "class-transformer": "0.3.1",
    "js-sha256": "^0.11.0",
    "npm": "^10.8.1",
    "prop-types": "^15.7.2",
    "qs": "^6.12.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "url-join": "^4.0.1",
    "url-parse": "^1.4.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@types/firefox-webext-browser": "^58.0.4",
    "@types/qs": "^6.9.15",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/url-join": "^0.8.3",
    "babel-loader": "^8.0.6",
    "no-console-webpack-plugin": "0.0.5",
    "ts-loader": "^4.5.0",
    "typescript": "^5.5.3",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "web-ext": "^2.9.3",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.1.2"
  }
}
